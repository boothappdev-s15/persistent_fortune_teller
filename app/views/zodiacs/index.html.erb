<h1>List of Zodiac</h1>

<table class="table table-striped table-hover">
  <tr>
    <th>Sign</th>
    <th>Creature</th>
    <th>Actions</th>
  </tr>

  <% @list_of_zodiacs.each do |giraffe| %>
    <tr>
      <td><%= giraffe.sign %></td>
      <td><%= giraffe.creature %></td>
      <td><a href="http://localhost:3000/delete_zodiac/<%= giraffe.id %>">Delete</a></td>
    </tr>
  <% end %>
</table>
